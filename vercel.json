{
    "buildCommand": "cd apps/web && npm run build",
    "outputDirectory": "apps/web/dist",
    "installCommand": "npm install && cd apps/web && npm install",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/landing/(.*)",
            "destination": "/landing/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}